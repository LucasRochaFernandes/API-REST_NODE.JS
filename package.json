{
  "name": "api_rest",
  "version": "1.0.0",
  "description": "Gympass style app",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit:watch": "vitest --dir src/use-cases",
    "test:e2e:watch": "vitest --dir src/http",
    "test:unit": "vitest run --dir src/use-cases",
    "test:e2e": "vitest run src/http",
    "test:ui:unit": "vitest --ui --dir src/use-cases",
    "test:ui:e2e": "vitest --ui --dir src/http"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.10.5",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.1.0",
    "@vitest/ui": "1.1.0",
    "eslint": "8.56.0",
    "prisma": "5.7.1",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.1.0"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.4",
    "@prisma/client": "5.7.1",
